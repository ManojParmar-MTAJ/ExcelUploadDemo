<h3>Users From Database</h3>
<table border=1>
  <thead>
    <tr>
      <th>ID</th>
      <th>FISRT NAME</th>
      <th>LAST NAME</th>
      <th>EMAIL</th>
      <th>PASSWORD</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user|%>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
        <td><%= user.password %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</br>
</br>
<%= link_to 'upload file', root_path %>
